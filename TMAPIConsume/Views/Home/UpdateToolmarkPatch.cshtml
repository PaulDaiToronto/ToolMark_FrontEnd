@model Toolmark
@{
    Layout = "_Layout"; ViewBag.Title = "Update a workcase from PATCH request";
}

<h2>Update a workcase from Patch request<a asp-action="Index" class="btn btn-sm btn-secondary">Back</a></h2>
<form method="post">
    <div class="form-group">
        <label asp-for="CaseNumber">Case Number</label>
        <input class="form-control" asp-for="CaseNumber" />
    </div>
    <div class="form-group">
        <label asp-for="Category">Category</label>
        <input class="form-control" asp-for="Category" />
    </div>

    <div class="form-group">
        <label asp-for="Email">Email</label>
        <input class="form-control" asp-for="Email" />
    </div>

    <div class="form-group">
        <label asp-for="ImageFileName">Image name</label>
        <input class="form-control" asp-for="ImageFileName" />
    </div>

    <div class="form-group">
        <label asp-for="Note">Note</label>
        <input class="form-control" asp-for="Note" />
    </div>

    <div class="text-center panel-body">
        <button type="submit" class="btn btn-sm btn-primary">Update</button>
    </div>
</form>